{
  "version": 2,
  "builds": [
    {
      "src": "api/flask.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/flask.py"
    }
  ],
  "build": {
    "env": {
      "VIRTUAL_ENV": "/tmp/virtualenv"
    },
    "scripts": {
      "postinstall": "pip install distutils"
    }
  }
}
